"404 Not found\");<br \/>\nresponse.end();<br \/>\n}<br \/>\n}<br \/>\nexports.route = route;<br \/>\n<\/code><\/p>\n<p>requestHandlers.js<br \/>\n<code><br \/>\nvar querystring = require(\"querystring\");<br \/>\nfunction start(response, postData) {<br \/>\nconsole.log(\"Request handler 'start' was called.\");<br \/>\n<\/code><br \/>\n<a href=\"http:\/\/55u.me\/wp-content\/uploads\/2012\/12\/nodejs.jpg\"><img class=\"alignleft size-medium wp-image-1076\" alt=\"node\" src=\"http:\/\/55u.me\/wp-content\/uploads\/2012\/12\/nodejs-300x151.jpg\" width=\"300\" height=\"151\" \/><\/a><\/p>\n<p>&nbsp;<\/p>\n<p>&nbsp;<br \/>\n&nbsp;<br \/>\n&nbsp;<\/p>\n<p>&nbsp;<br \/>\n<code><br \/>\nresponse.writeHead(200, {\"Content-Type\": \"text\/html\"});<br \/>\nresponse.write(body);<br \/>\nresponse.end();<br \/>\n}<br \/>\nfunction upload(response, postData) {<br \/>\nconsole.log(\"Request handler 'upload' was called.\");<br \/>\nresponse.writeHead(200, {\"Content-Type\": \"text\/plain\"});<br \/>\nresponse.write(\"You've sent the text: \"+<br \/>\nquerystring.parse(postData).text);<br \/>\nresponse.end();<br \/>\n}<\/p>\n<p>exports.start = start;<br \/>\nexports.upload = upload;<\/code><\/p>\n<p>\u6d4b\u8bd5\u5148node index.js \u8bbf\u95ee\u7aef\u53e3127.0.0.1:8888<\/p>\n","excerpt":"index.js var server = require(\".\/server\"); var router = [...]","date":"2012-12-27 14:30:28","modified":"2012-12-28 23:07:28","categories":[{"id":13,"slug":"work","title":"\u5199\u4ee3\u7801\u7684\u65e5\u5b50","description":"","parent":0,"post_count":74}],"tags":[{"id":55,"slug":"js","title":"js","description":"","post_count":7},{"id":72,"slug":"nodejs","title":"nodejs","description":"","post_count":1}],"author":{"id":1,"slug":"guanqunyin","name":"guanqun yin","first_name":"","last_name":"","nickname":"guanqun yin","url":"","description":""},"comments":[],"attachments":[{"id":1076,"url":"http:\/\/55u.me\/wp-content\/uploads\/2012\/12\/nodejs.jpg","slug":"nodejs","title":"nodejs","description":"","c